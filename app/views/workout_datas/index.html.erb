<div class='page-header'>
  <h1>Your Current Workout</h1>
</div>

<table class='table table-striped table-hover'>
  <thead>
    <tr>
      <th>Date</th>
          
    </tr>
  </thead>

  <tbody>
    <% @workout_datas.each do |workout_data| %>
      <tr>
        <td><%= link_to workout_data.date, workout_data_path(workout_data) %></td>
        <td><%= link_to 'Edit', edit_workout_data_path(workout_data) %> | 
            <%= link_to 'Destroy', workout_data_path(workout_data), method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Workout Data', new_workout_data_path, class: 'btn btn-primary' %>
        