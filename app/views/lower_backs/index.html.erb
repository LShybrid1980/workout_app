<div class='page-header'>
  <h1>Your Current Lower Back Workout</h1>
</div>

<table class='table table-striped table-hover'>
  <thead>
    <tr>
      <th>Name</th>
      <th>Weights</th>
      <th>Sets</th>
      <th>Reps</th>    
    </tr>
  </thead>

  <tbody>
    <% @lower_backs.each do |lower_back| %>
      <tr>
        <td><%= link_to lower_back.status_type, lower_back %></td>
        <td><%= link_to lower_back.weight, lower_back %></td>
        <td><%= link_to lower_back.set, lower_back %></td>
        <td><%= link_to lower_back.rep, lower_back %></td>
        <td><%= link_to 'Edit', edit_workout_data_lower_back_path(lower_back) %> | 
            <%= link_to 'Destroy', lower_back, method: :delete, data: { confirm: 'Are you sure?' } %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Lower Backs Workout Data', new_workout_data_lower_back_path, class: 'btn btn-primary' %>
        
        
        
        
